{
  "manifest_version": 2,
  "name": "Nameless Live",
  "version": "0.1.6",

  "description": "Permet de savoir si Nameless est en Live",
  "icons": {
    "16": "nameless-16.png",
    "128": "nameless-128.png"
  },

  "browser_action": {
    "default_icon": "nameless-19.png",
    "default_title": "Nameless Live"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "https://api.twitch.tv/",
    "notifications"
  ],

  "background": {
    "scripts": ["background.js"]
  }

}